/* pages/edit_people/index.wxss */
.tab{
    height: 7vh;
    display: flex;
    line-height: 7vh;
    border-bottom: 2rpx solid #EAEAEA;
    margin-bottom: 56rpx;
  }
  
  .tab view{
    flex: 1 0 auto;
    font-size: 24rpx;
    color: #A8A8A8;
    text-align: center;
  }
  .tab .active{
    color: #373737;
    font-size: 28rpx;
    border-bottom: 4rpx solid #EB544D;
    padding-bottom:1.8vh;
  }
  .list{
    height: 93vh;
  }
  
  .item{
    display: flex;
    background: url(data:image/png;base64,) no-repeat;
    background-size: cover;
    width:702rpx;
  height:185rpx;
  
   margin: 0 auto 20rpx;
  }
  
  .item .left{
    width: 242rpx;
   
    text-align: center;
  
    font-size: 80rpx;
  
    color: #FFFFFF;
    display: flex;align-items: center;justify-content: center;
  
  }
  .item .left .zhekou{
    float: left;
  justify-content: left;
  display: flex;
  flex-direction: column;
  
  align-items: flex-start;
  margin-left: 10rpx;
  }
  .item .left .zhekou text{
  font-weight: normal;
  }
  .item .left text{
    color: #FFFFFF;       
  }
  .item .left text:first-child{
    font-size: 30rpx;
  }
  .item .left text:last-child{
    font-size: 25rpx;
  }
  .mone_fuhao{ font-size: 40rpx!important;font-weight: normal; margin-top: 20rpx;}
  
  .item .right{
   display: flex;
    flex-direction: column;
  justify-content: center;
  
   align-items: flex-start;
    padding-left: 50rpx;
    height: 100%;
  
    width: 410rpx;
  }
  .item .right .jieshi_quan{
    font-size:30rpx;
  font-family:PingFangSC;
  font-weight:blod;
  color: #373737;
  line-height: 70rpx;
  }
  .item .right .state{font-size:24rpx;font-weight:500;color: #373737;line-height: 50rpx;}
  