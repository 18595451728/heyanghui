
page {
  background: #f0f1f4;
  padding-bottom: 100rpx;
  box-sizing: border-box;
  position: relative;
}

.big {
  width: 100%;
  height: auto;
}

.banner {
  width: 100%;
  height: 748rpx;
  position: relative;
}

.jiaobiao {
  width: 83rpx;
  height: 37rpx;
  background: rgba(0, 0, 0, 1);
  border-radius: 19rpx;
  font-size: 20rpx;
  color: white;
  text-align: center;
  position: absolute;
  right: 20rpx;
  bottom: 30rpx;
  line-height: 37rpx;
  letter-spacing: 1px;
}

swiper, swiper image {
  width: 100%;
  height: 100%;
}

.detail_name {
  width: 90%;
  margin: 30rpx auto;
  height: 100%;
}

.detail_name_out {
  width: 100%;
  height: 200rpx;
  background: #fff;
  border: 1rpx solid transparent;
  box-sizing: border-box;
}

.detail_name>view {
  float: left;
}

.detail_name_right {
  width: 20%;
  height: 100%;
}

.detail_name_left {
  width: 80%;
}

.detail_name_left_top {
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
  white-space: normal;
  overflow: hidden;
  text-overflow: ellipsis;
}

.detail_name_left_bottom>text {
  display: inline-block;
  line-height: 30rpx;
}

.price1 {
  font-size: 22rpx;
  font-family: PingFangSC;
  font-weight: bold;
  color: rgba(234, 75, 67, 1);
  margin-right: 10rpx;
}

.price2 {
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: bold;
  color: rgba(234, 75, 67, 1);
}

.detail_name_right_top {
  width: 66rpx;
  height: 50rpx;
  margin-left: 80rpx;
}

.detail_name_right_bottom {
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(182, 182, 182, 1);
  text-align: right;
  margin-top: 10rpx;
  width: 100%;
}

.peizhi {
  width: 90%;
  height: 100%;
  margin: 20rpx auto;
}

.peizhi_item {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
  text-align: left;
  margin-top: 10rpx;
}

.guige_img {
  width: 15rpx;
  height: 25rpx;
  float: right;
  display: inline-block;
  margin-top: 5rpx;
  margin-right: 10rpx;
}

.peizhi_item>view {
  display: inline-block;
}

.peizhi_out {
  width: 100%;
  height: 210rpx;
  background: #fff;
  border: 1rpx solid transparent;
  margin: 30rpx 0;
  box-sizing: border-box;
}

.pingjia_out {
  width: 100%;
  height: auto;
  background: #fff;
  border: 1rpx solid transparent;
  margin: 30rpx 0;
  box-sizing: border-box;
  padding-bottom: 30rpx;
}

.pingjia {
  width: 90%;
  height: 100%;
  margin: 0 auto;
}

.pingjia_top {
  width: 100%;
  height: auto;
  margin-top: 30rpx;
  border-bottom: 2rpx solid rgba(178, 175, 163, 0.31);
  padding-bottom: 60rpx;
  box-sizing: border-box;
  clear: both;
}

.pingjia_top>view {
  display: inline-block;
}

.pingjia_top_left {
  float: left;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
}

.pingjia_top_right {
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(121, 121, 121, 1);
  float: right;
}

.pingjia_arrow {
  width: 15rpx;
  height: 25rpx;
  vertical-align: middle;
  margin-top: -5rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
}

.pingjia_center {
  width: 100%;
  height: auto;
  margin-top: 30rpx;
  border-bottom: 2rpx solid rgba(178, 175, 163, 0.31);
  padding-bottom: 30rpx;
}

.pingjia_center>view {
  display: inline-block;
}

.pingjia_center_item {
  width: 120rpx;
  height: 46rpx;
  background: rgba(253, 245, 245, 1);
  border-radius: 10rpx;
  text-align: center;
  line-height: 46rpx;
  font-size: 22rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(85, 85, 85, 1);
  margin-right: 20rpx;
}

.pingjia_bottom {
  width: 100%;
  height: auto;
  margin-top: 30rpx;
}

.pingjia_bottom1 {
  width: 100%;
  height: 80rpx;
}

.pingjia_bottom1>view {
  display: inline-block;
}

.pingjia_bottom1_left {
  float: left;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(62, 62, 62, 1);
  line-height: 60rpx;
}

.pingjia_bottom1_right {
  float: right;
  margin-right: 50rpx;
  margin-top: 10rpx;
}

.pingjia_bottom1_left_img {
  width: 70rpx;
  height: 60rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.pingjia_bottom1_right_img {
  width: 30rpx;
  height: 25rpx;
}

.pingjia_bottom2 {
  width: 100%;
  font-size: 24rpx;
  height: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(134, 134, 134, 1);
}

.shop_out {
  width: 100%;
  height: auto;
  background: #fff;
  margin-top: 10rpx;
  border: 1rpx solid transparent;
  box-sizing: border-box;
}

.shop {
  width: 90%;
  margin: 30rpx auto;
}

.shop_title {
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
  text-align: center;
  padding-bottom: 30rpx;
  border-bottom: 2rpx solid rgba(178, 175, 163, 0.31);
  margin-top: 30rpx;
}

.shop_img {
  width: 100%;
  height: auto;
  margin-top: 30rpx;
}

.shop_img_item {
  width: 85%;
  height: 300rpx;
  margin: 0 auto 30rpx;
}

.shop_img_item_img {
  width: 100%;
  height: 100%;
}

.dibu {
  width: 100%;
  height: 101rpx;
  position: fixed;
  z-index: 999;
  bottom: 0;
  background: #fff;
  box-shadow: 0px -10rpx 38rpx 0rpx rgba(97, 97, 97, 0.1);
  box-sizing: border-box;
}

.dibu>view {
  float: left;
}

.dibu_logo {
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin: 15rpx auto 0;
}

.dibu_text {
  font-size: 22rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(96, 96, 96, 1);
  text-align: center;
}

.dibu1, .dibu2 {
  width: 15%;
  height: 100%;
}

.dibu3, .dibu4 {
  width: 35%;
  height: 100%;
  text-align: center;
  line-height: 101rpx;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.dibu3 {
  background: #ef8e60;
}

.dibu4 {
  background: #eb544d;
}

.dibu_peisong {
  height: auto;
  width: 100%;
  position: fixed;
  z-index: 1000;
  background: #fff;
  bottom: -500rpx;
  transition: all 3s;
}

.dibu_peisong_top {
  height: 700rpx;
  width: 100%;
  background: #fff;
}

.dibu_peisong_bottom {
  height: 100rpx;
  background: rgba(235, 84, 77, 1);
  line-height: 100rpx;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  text-align: center;
}

.dibu_peisong_top_title {
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
  line-height: 44rpx;
  margin: 30rpx auto 50rpx;
}

.dibu_peisong_top_list {
  width: 90%;
  margin: 0 auto;
}

.dibu_peisong_top_item {
  width: 100%;
  margin: 0 auto 30rpx;
  height: auto;
  padding-bottom: 30rpx;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
  border-bottom: 1rpx solid rgba(236, 236, 236, 1);
  box-sizing: border-box;
}

.dibu_guige {
  width: 100%;
  height: auto;
  background: #fff;
  position: fixed;
  z-index: 1000;
  bottom: -300rpx;
  transition: all 3s;
}

.dibu_guige_top {
  width: 90%;
  height: auto;
  margin: 0 auto;
}

.dibu_guige_top1 {
  width: 100%;
  height: 140rpx;
  border-bottom: 1rpx solid rgba(236, 236, 236, 1);
  padding-bottom: 10rpx;
}

.dibu_guige_top1>view {
  float: left;
}

.dibu_guige_top11 {
  position: relative;
  width: 168rpx;
  height: 168rpx;
}

.dibu_guige_top11>image {
  width: 168rpx;
  height: 168rpx;
  display: block;
  border-radius: 10px;
  position: absolute;
  top: -50rpx;
}

.dibu_guige_top12 {
  width: 400rpx;
  height: auto;
  margin: 25rpx;
}

.dibu_guige_top12_title {
  width: 100%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
}

.dibu_guige_top12_price>text {
  display: inline-block;
  font-family: PingFangSC;
  font-weight: bold;
  color: rgba(234, 75, 67, 1);
  line-height: 20rpx;
}

.dibu_guige_top12_price1 {
  font-size: 22rpx;
}

.dibu_guige_top12_price2 {
  font-size: 26rpx;
}

.close {
  width: 50rpx;
  height: 45rpx;
  margin-top: 20rpx;
}

.dibu_guige_top2 {
  width: 100%;
  height: auto;
  clear: both;
  border-bottom: 1rpx solid rgba(236, 236, 236, 1);
  padding-bottom: 30rpx;
}

.dibu_guige_top2_title {
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(66, 66, 66, 1);
  clear: both;
  margin: 30rpx 0;
}
.guige{
  display: flex;
}
.dibu_guige_top2_title_name {
  width: 250rpx;
  height: 60rpx;
  background:rgba(247,248,249,1);
  border-radius: 10rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #333;
  margin-right: 20rpx;
}
.dibu_guige_top2_title_name.active{
  background:rgba(235,84,77,1);
  color:rgba(255,255,255,1);
}
.dibu_guige_top3 {
  width: 100%;
  height: auto;
}

.dibu_guige_top3_left {
  width: 70%;
  height: 150rpx;
  clear: both;
}

.dibu_guige_top3_right {
  width: auto;
  height: auto;
}

.dibu_guige_top3>view {
  float: left;
}

.dibu_guige_top2_shengyu {
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(183, 183, 183, 1);
}

.dibu_guige_top3_right_yuansuan {
  width: auto;
  height: 55rpx;
  margin-top: 30rpx;
}

.dibu_guige_top3_right_yuansuan>view {
  width: 60rpx;
  height: 60rpx;
  background: rgba(247, 248, 249, 1);
  border-radius: 10rpx 0px 0px 10rpx;
  line-height: 60rpx;
  text-align: center;
  background: #f7f8f9;
  display: inline-block;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(183, 183, 183, 1);
}

.dibu_guige_top3_right_yuansuan>view:nth-child(2) {
  margin: 0 10rpx;
  color: rgba(56, 56, 56, 1) !important;
}

.dibu_guige_bottom {
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  clear: both;
}

.dibu_guige_bottom1 {
  width: 50%;
  height: 100%;
  background: rgba(239, 142, 96, 1);
  float: left;
}

.dibu_guige_bottom2 {
  width: 50%;
  height: 100%;
  background: rgba(235, 84, 77, 1);
  float: left;
}

.zhegai {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  z-index: 998;
  left: 0;
  top: 0;
}
