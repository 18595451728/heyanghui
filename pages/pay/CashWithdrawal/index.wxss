page {
  background: #f9f9f9;
}
.CashWithdrawal {
  width: 100%;
}
.CashWithdrawal_main {
  width: 725rpx;
  height: 754rpx;
  margin: 100rpx 12.5rpx;
  position: relative;
}
.CashWithdrawal_main image {
  width: 100%;
  height: 100%;
}
.CashWithdrawal_main_one{
  position: absolute;
  top: 0;
  left: 63rpx;
  width: 606rpx; 
  height: 535rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-bottom:2px dashed rgba(183,183,183,1);
}
.CashWithdrawal_main_one_img{
  width:215rpx;
height:176rpx;display: inline-block;
margin-top:85rpx;
}
.CashWithdrawal_main_one text:nth-child(2){
  font-size:35rpx;
font-family:PingFangSC;
font-weight:500;
color:rgba(44,43,43,1);
line-height:39rpx;
margin-top: 28rpx;
}
.CashWithdrawal_main_one text:last-child{
width:478rpx;
font-size:25rpx;
font-family:PingFangSC;
font-weight:500;
color:rgba(144,144,144,1);
line-height:39rpx;
text-align: center;
margin-top: 40rpx;
}
.CashWithdrawal_main_two{
  position: absolute;bottom: 0;left: 0;width: 100%;height: 219rpx;
  margin-top: 56rpx; 
}
.CashWithdrawal_main_two_box{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 583rpx;
  margin: 0 auto;
}
.CashWithdrawal_main_two_box:first-child{
 margin-top: 50rpx;
}
.CashWithdrawal_main_two_box text{
font-size:28rpx;
font-family:PingFangSC;
font-weight:500;
color:rgba(43,43,43,1);
line-height:39rpx;
}
.CashWithdrawal_main_two_box text:last-child{
color:rgba(105,105,105,1);
line-height:39rpx;
}

.CashWithdrawal_main_two_box:first-child .CashWithdrawal_main_two_text{
display: flex;
justify-content: start;
}
.CashWithdrawal_main_two_box:first-child .CashWithdrawal_main_two_text view{
  width:42rpx;
height:44rpx;
}
.CashWithdrawal_main_two_box:first-child .CashWithdrawal_main_two_text view images{
  width:100%;
height:100%;
}
.CashWithdrawal_main_three{
  width:671rpx;
  height:90rpx;
  line-height: 90rpx;
  background:rgba(235,84,77,1);
  border-radius:10px;
  color: #ffffff;
  text-align: center;
  margin: 170rpx auto 0;
}
