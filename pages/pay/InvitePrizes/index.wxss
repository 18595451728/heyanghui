page {
  background: #fff;
}

.InvitePrizes {
  width: 100%;
}

.InvitePrizes_main_one {
  width: 738rpx;
  height: 389rpx;
  border-radius: 20rpx;
  margin: 53rpx auto 0;
}

.InvitePrizes_main_one image {
  width: 100%;
  height: 100%;
}

.InvitePrizes_main_two {
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 564rpx;
}

.InvitePrizes_main_two_img {
  width: 112rpx;
  height: 112rpx;
  border: 1rpx solid rgba(255, 255, 255, 1);
  margin-top: 32rpx;
}

.InvitePrizes_main_two_img image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.InvitePrizes_main_two_tel {
  width: 214rpx;
  height: 29rpx;
  font-size: 34rpx;
  font-family: HYYakuHei;
  font-weight: normal;
  color: rgba(92, 92, 92, 1);
  line-height: 26rpx;
  margin: 20rpx auto 65rpx;
}

.InvitePrizes_main_two_goods {
  width: 450rpx;
  /* height: 69rpx; */
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(165, 165, 165, 1);
  line-height: 44rpx;
}

.InvitePrizes_main_three {
  display: flex;
  justify-content: space-between;
  width: 698rpx;
  margin: 0 auto;
}

.InvitePrizes_main_three view {
  width: 338rpx;
  height: 86rpx;
  background: linear-gradient(-90deg, rgba(251, 119, 108, 1), rgba(229, 80, 73, 1));
  text-align: center;
  line-height: 86rpx;
  font-size: 31rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
}

.InvitePrizes_main_four {
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(233, 87, 79, 1);
  margin: 48rpx auto 85rpx;
}
