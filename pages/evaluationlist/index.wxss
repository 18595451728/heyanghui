page {
  background-color: #f0f1f4;
}

.header {
  width: 710rpx;
  height: 119rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  background: rgba(255, 255, 255, 1);
  display: flex;
  align-items: center;
  font-size: 25rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(236, 92, 86, 1);
  line-height: 56rpx;
  text-align: center;
  margin-bottom: 20rpx;
}



.item {
  width: 131rpx;
  height: 56rpx;
  background: rgba(253, 245, 245, 1);
  border-radius: 28rpx;
  color: #4b4b4b;
  margin-left: 20rpx;
   border: 2rpx solid #fff;
}
.item.all {
  width: 131rpx;
  height: 56rpx;
  background: rgba(253, 245, 245, 1);
  border: 2rpx solid rgba(233, 68, 60, 1);
  border-radius: 28rpx;
}
.list {
  width: 710rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  background: rgba(255, 255, 255, 1);
  display: flex;
  flex-direction: column;
  border-bottom: 1px solid #f0f1f4;
}

.listheader {
  width: 100%;
  height: 76rpx;
  padding-top: 37rpx;
  display: flex;
  justify-content: space-between;
}

.userlogo {
  width: 78rpx;
  height: 76rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.headermsg {
  width: 620rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.phone {
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 27rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(62, 62, 62, 1);
  line-height: 44rpx;
}

.scores {
  width: 26rpx;
  height: 23rpx;
  margin-left: 2rpx;
}

.usermsg {
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(134, 134, 134, 1);
  line-height: 44rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.foot {
  width: 100%;
  height: 40rpx;
  margin-bottom: 40rpx;
  display: flex;
  justify-content: space-between;
}

.title {
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(194, 194, 194, 1);
  line-height: 40rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.msgimg {
  width: 35rpx;
  height: 29rpx;
  margin-left: 10rpx;
}

.zanimg {
  width: 37rpx;
  height: 37rpx;
  margin-left: 10rpx;
}

.footitem {
  height: 100%;
  display: flex;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(78, 78, 78, 1);
  line-height: 40rpx;
}

.itemmsg {
  height: 100%;
  display: flex;
  align-items: center;
}
